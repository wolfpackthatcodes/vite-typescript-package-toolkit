{
  "name": "@wolfpackthatcodes/package-name",
  "description": "Package description",
  "version": "0.0.0",
  "license": "MIT",
  "homepage": "https://github.com/wolfpackthatcodes/vite-typescript-package-toolkit",
  "repository": {
    "type": "git",
    "url": "https://github.com/wolfpackthatcodes/vite-typescript-package-toolkit.git"
  },
  "bugs": {
    "url": "https://github.com/wolfpackthatcodes/vite-typescript-package-toolkit/issues/new/choose"
  },
  "author": {
    "name": "Luis Aveiro",
    "email": "support@luisaveiro.io"
  },
  "keywords": [
    "wolfpackthatcodes",
    "vite",
    "typescript",
    "package",
    "toolkit"
  ],
  "type": "module",
  "main": "./dist/package-name.umd.cjs",
  "module": "./dist/package-name.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/package-name.js",
      "require": "./dist/package-name.umd.cjs"
    }
  },
  "files": [
    "dist",
    "images",
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "yarn run lint --fix",
    "format:style": "prettier . --write",
    "test": "tsc && vitest watch",
    "test:ci": "tsc && vitest run",
    "test:coverage": "yarn run test --coverage",
    "test:ui": "yarn run test --ui --coverage --open=false"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.15.3",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.4",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">= 20"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
